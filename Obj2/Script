function ChangeSpawn()
	game.Workspace.Spawns.Spawn2.Neutral = false
	game.Workspace.Spawns.Spawn3.Neutral = false
	game.Workspace.Spawns.Spawn4.Neutral = false
	game.Workspace.Spawns.Spawn5.Neutral = false
	game.Workspace.Spawns.Spawn6.Neutral = false
	game.Workspace.Spawns.Spawn7.Neutral = false
	game.Workspace.Spawns.Spawn8.Neutral = false
	game.Workspace.Spawns.Spawn9.Neutral = false
	game.Workspace.Spawns.Spawn10.Neutral = false
	game.Workspace.Spawns.Spawn12.Neutral = false
	game.Workspace.Spawns.Spawn13.Neutral = false
	game.Workspace.Spawns.Spawn14.Neutral = true
	game.Workspace.Spawns.Spawn15.Neutral = false
	game.Workspace.Spawns.SpawnReallyFinal.Neutral = false
	game.Workspace.Spawns.SpawnFinal.Neutral = false
	game.Workspace.Spawns.Spectate.Neutral = false
	game.Workspace.Spawns.StartSpawn.Neutral = false
end

script.Parent.Touched:Connect(function(hit)
	hit.Parent:FindFirstChild("Humanoid")
	ChangeSpawn()
	script.Parent.CanTouch = false
	game.ReplicatedStorage.TextFolders.obj2:FireAllClients(hit)
end)

